DEF	ma Script {
	eventIn SFBool turnOn
	eventOut SFBool On
	eventOut SFBool Off
	field SFBool turn FALSE
	url "javascript:
					function turnOn(val){
								if(val){
								turn = !turn;
								if(turn)
								{
								On= TRUE;
                        		Off=FALSE;
            					}
								else
        						{
									On=FALSE;
									Off=TRUE;
    							}

            					}

    								}

    "
}
ROUTE touch1.isActive TO	ma.turnOn
ROUTE ma.On	TO time1.enabled

ROUTE time1.fraction_changed TO	orient.set_fraction
ROUTE orient.value_changed TO trong.rotation
ROUTE touch1.touchTime TO time1.startTime
